<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1" 
    android:background="#ebebeb">

    <TableRow android:layout_weight="1">

        <org.linphone.ui.Digit
            android:id="@+id/Digit1"
            style="@style/DialerDigit"
            android:background="@drawable/diler_1_1_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:text="1" />

        <org.linphone.ui.Digit
            android:id="@+id/Digit2"
            style="@style/DialerDigit"
            android:background="@drawable/diler_2_2_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:text="2" />

        <org.linphone.ui.Digit
            android:id="@+id/Digit3"
            style="@style/DialerDigit"
            android:background="@drawable/diler_3_3_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:text="3" />
    </TableRow>

    <TableRow android:layout_weight="1" >

        <org.linphone.ui.Digit
            android:id="@+id/Digit4"
            style="@style/DialerDigit"
            android:background="@drawable/diler_4_4_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:text="4" />

        <org.linphone.ui.Digit
            android:id="@+id/Digit5"
            style="@style/DialerDigit"
            android:background="@drawable/diler_5_5_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:text="5" />

        <org.linphone.ui.Digit
            android:id="@+id/Digit6"
            style="@style/DialerDigit"
            android:background="@drawable/diler_6_6_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:text="6" />
    </TableRow>

    <TableRow android:layout_weight="1" >

        <org.linphone.ui.Digit
            android:id="@+id/Digit7"
            style="@style/DialerDigit"
            android:background="@drawable/diler_7_7_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:text="7" />

        <org.linphone.ui.Digit
            android:id="@+id/Digit8"
            style="@style/DialerDigit"
            android:background="@drawable/diler_8_8_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:text="8" />

        <org.linphone.ui.Digit
            android:id="@+id/Digit9"
            style="@style/DialerDigit"
            android:background="@drawable/diler_9_9_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:text="9" />
    </TableRow>

    <TableRow android:layout_weight="1" >

        <org.linphone.ui.Digit
            android:id="@+id/DigitStar"
            style="@style/DialerDigit"
            android:background="@drawable/diler_x_x_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:layout_marginBottom="1px"
            android:text="*" />

        <org.linphone.ui.Digit
            android:id="@+id/Digit00"
            style="@style/DialerDigit"
            android:background="@drawable/diler_0_0_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginRight="1px"
            android:layout_marginBottom="1px"
            android:text="0+" />

        <org.linphone.ui.Digit
            android:id="@+id/DigitHash"
            style="@style/DialerDigit"
            android:background="@drawable/diler_j_j_xml"
            android:soundEffectsEnabled="true"
            android:layout_marginTop="1px"
            android:layout_marginBottom="1px"
            android:text="#" />
    </TableRow>

</TableLayout><!-- android:layout_width="fill_parent" android:layout_height="fill_parent" android:textColor="@android:color/black" android:background="@drawable/clavier_bg" android:textStyle="bold" android:textSize="20sp" android:layout_weight="1" -->
