<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white" >

    <RelativeLayout
        android:id="@+id/cpkrl1"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@android:color/black"
        android:visibility="visible" >

        <ImageButton
            android:id="@+id/cpkib1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:background="@drawable/tab_contacts1"
            android:clickable="true"
            android:onClick="toContacts" />

        <ImageButton
            android:id="@+id/cpkib2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/ic_delete_character"
            android:text="" />
        <!-- android:background="@drawable/input_bg" -->

        <org.linphone.ui.AddressText
            android:id="@+id/cpket1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:layout_toLeftOf="@+id/cpkib2"
            android:layout_toRightOf="@id/cpkib1"
            android:background="@color/black"
            android:editable="true"
            android:ems="10"
            android:enabled="true"
            android:focusable="false"
            android:gravity="center"
            android:inputType="phone"
            android:text="sip:liuyiyi@sip.linphone.org"
            android:textColor="@color/white"
            android:textSize="24sp" >

            <requestFocus />
        </org.linphone.ui.AddressText>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/cpkrl2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/cpkrl1" >

        <ListView
            android:id="@+id/cpklv1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >
        </ListView>
    </RelativeLayout>

    <LinearLayout
