                    android:layout_centerInParent="true"
                    android:src="@drawable/trans" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_marginTop="1px"
            android:orientation="horizontal"
            android:padding="0dp" >

